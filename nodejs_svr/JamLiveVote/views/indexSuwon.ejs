                    <td>투표번호</td>
                    <td>시간</td>
                    <td>액션</td>
                </tr>
            </table>
        </div>
        <div id="btn-vote-list-close">
            닫기
        </div>
    </div>
    <div id="area-top">
        <div id="area-top-left">
            <div id="logo">
                <img src="/images/logoh.png"/>
            </div>
            <div id="serv-info">
                <img class='icon' src="/images/cnt_global_icon.png"/>
                <span class="info" id="total-conn-cnt">0</span>
                <img class='icon' src="/images/cnt_local_icon.png"/>
