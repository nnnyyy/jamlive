        if( isMobile() ) {
            window.location.href = 'http://jamlive.net';
            return;
        }

        var stringTable = {
            "modify-hint": "<%=__('modify-hint')%>",
            "modify-hint-complete": "<%=__('modify-hint-complete')%>",
            "today": "<%=__('today')%>",
            "hour": "<%=__('hour')%>",
            "minute": "<%=__('minute')%>",
            "quiz-on": "<%=__('quiz-on')%>",
            "quiz-off": "<%=__('quiz-off')%>"
        }

        CKEDITOR.replace('memo-area');
        var socket = io();
        init( socket, stringTable );
    });
</script>
