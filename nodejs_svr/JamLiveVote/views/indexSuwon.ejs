            <img style="width: 20px; height: 20px;" src="/images/star3.png"/>
            <img style="width: 20px; height: 20px;" src="/images/star4.png"/>
            <img style="width: 20px; height: 20px;" src="/images/star5.png"/>
            <img style="width: 20px; height: 20px;" src="/images/star6.png"/>
            <img style="width: 20px; height: 20px;" src="/images/star7.png"/>
            <img style="width: 20px; height: 20px;" src="/images/star8.png"/>
            <img style="width: 20px; height: 20px;" src="/images/star9.png"/>
            <img style="width: 20px; height: 20px;" src="/images/star10.png"/>
